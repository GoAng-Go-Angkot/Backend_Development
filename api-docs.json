  "components": {
    "schemas": {
      "DriverRegisterRequest": {
        "required": [
          "email",
          "username",
          "route",
          "password"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "example": "Budi Santoso"
          },
          "email": {
            "type": "string",
            "format": "email",
            "example": "budi.driver@example.com"
          },
          "password": {
            "type": "string",
            "format": "password",
            "example": "strongpassword123"
          },
          "route": {
            "type": "string",
            "example": "05"
          }
        }
      },
      "DriverRegisterResponse": {
        "type": "object",
        "properties": {
          "data": {
            "$ref": "#/components/schemas/DriverRegisterResponse_data"
          }
        }
      },
      "DriverLoginRequest": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
