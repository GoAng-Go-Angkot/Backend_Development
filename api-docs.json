      "post": {
        "tags": [
          "Driver"
        ],
        "summary": "Register a new driver",
        "operationId": "registerDriver",
        "requestBody": {
          "description": "Driver registration details including assigned Angkot number",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DriverRegisterRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Driver successfully registered",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DriverRegisterResponse"
                }
              }
            }
          },
          "400": {
            "description": "Validation error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse400DriverRegister"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse500"
                }
              }
            }
          }
        }
      }
    },
